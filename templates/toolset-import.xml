<?xml version="1.0" encoding="UTF-8"?>
<views>
  <views>
    <view>
      <post_content>[wpv-filter-meta-html][wpv-layout-meta-html]</post_content>
      <post_title>{{title}}</post_title>
      <post_excerpt></post_excerpt>
      <post_type>view</post_type>
      <post_status>publish</post_status>
      <meta>
        <_wpv_settings>
          <view-query-mode>normal</view-query-mode>
          <view_description></view_description>
          <view_purpose>all</view_purpose>
          <query_type>
            <state>posts</state>
          </query_type>
          <post_type_dont_include_current_page>1</post_type_dont_include_current_page>
          <taxonomy_hide_empty>1</taxonomy_hide_empty>
          <taxonomy_include_non_empty_decendants>1</taxonomy_include_non_empty_decendants>
          <taxonomy_pad_counts>1</taxonomy_pad_counts>
          <orderby>post_date</orderby>
          <order>DESC</order>
          <orderby_second></orderby_second>
          <order_second>DESC</order_second>
          <taxonomy_orderby>name</taxonomy_orderby>
          <taxonomy_order>DESC</taxonomy_order>
          <users_orderby>user_login</users_orderby>
          <users_order>ASC</users_order>
          <limit>-1</limit>
          <offset>0</offset>
          <taxonomy_limit>-1</taxonomy_limit>
          <taxonomy_offset>0</taxonomy_offset>
          <users_limit>-1</users_limit>
          <users_offset>0</users_offset>
          <pagination>
            <type>disabled</type>
            <posts_per_page>10</posts_per_page>
            <effect>fade</effect>
            <duration>500</duration>
            <speed>5</speed>
            <preload_images>1</preload_images>
            <cache_pages>1</cache_pages>
            <preload_pages>1</preload_pages>
            <pre_reach>1</pre_reach>
            <spinner>default</spinner>
            <spinner_image_uploaded></spinner_image_uploaded>
            <callback_next></callback_next>
          </pagination>
          <filter_meta_html>[wpv-filter-start hide=&quot;false&quot;][wpv-filter-controls][/wpv-filter-controls][wpv-filter-end]</filter_meta_html>
          <filter_meta_html_css></filter_meta_html_css>
          <filter_meta_html_js></filter_meta_html_js>
          <layout_meta_html_state>
            <html>on</html>
            <css>off</css>
            <js>off</js>
            <img>off</img>
          </layout_meta_html_state>
          <layout_meta_html_css>{{css}}</layout_meta_html_css>
          <layout_meta_html_js>/**
 * {{title}}
 * @version {{version}}
 * @link {{link}}
 * @author {{author}}
 * @licence {{license}}
 */
 {{js}}</layout_meta_html_js>
          <sections-show-hide>
            <pagination>off</pagination>
            <filter-extra-parametric>off</filter-extra-parametric>
            <filter-extra>off</filter-extra>
          </sections-show-hide>
          <post_types>
            <post_type>post</post_type>
          </post_types>
          <taxonomy_types>
            <taxonomy_type>category</taxonomy_type>
          </taxonomy_types>
          <roles_types>
            <roles_type>administrator</roles_type>
          </roles_types>
        </_wpv_settings>
        <_wpv_layout_settings>
          <additional_js></additional_js>
          <layout_meta_html>{{html}}</layout_meta_html>
          <style>unformatted</style>
          <table_cols>2</table_cols>
          <bootstrap_grid_cols>1</bootstrap_grid_cols>
          <bootstrap_grid_container></bootstrap_grid_container>
          <bootstrap_grid_row_class></bootstrap_grid_row_class>
          <bootstrap_grid_individual></bootstrap_grid_individual>
          <include_field_names></include_field_names>
        </_wpv_layout_settings>
      </meta>
    </view>
  </views>
</views>